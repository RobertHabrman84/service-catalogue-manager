# Development Environment Configuration

# API Configuration
VITE_API_BASE_URL=http://localhost:7071/api
VITE_API_TIMEOUT=30000

# Azure AD Authentication (Development)
VITE_AZURE_AD_CLIENT_ID=dev-client-id
VITE_AZURE_AD_TENANT_ID=dev-tenant-id
VITE_AZURE_AD_REDIRECT_URI=http://localhost:5173
VITE_AZURE_AD_SCOPE=api://dev-api-scope/.default

