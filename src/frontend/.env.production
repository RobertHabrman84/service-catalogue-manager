# Production Environment Configuration

# API Configuration
VITE_API_BASE_URL=https://func-scm-prod-weu.azurewebsites.net/api
VITE_API_TIMEOUT=30000

# Azure AD Authentication (Production)
VITE_AZURE_AD_CLIENT_ID=${AZURE_AD_CLIENT_ID}
VITE_AZURE_AD_TENANT_ID=${AZURE_AD_TENANT_ID}
VITE_AZURE_AD_REDIRECT_URI=https://scm.example.com
VITE_AZURE_AD_SCOPE=api://${AZURE_AD_CLIENT_ID}/.default

# Feature Flags
VITE_FEATURE_EXPORT_PDF=true
VITE_FEATURE_EXPORT_MARKDOWN=true
VITE_FEATURE_UUBOOKKIT=true
VITE_FEATURE_DARK_MODE=false

# Application Settings
VITE_APP_NAME=Service Catalogue Manager
VITE_APP_VERSION=1.0.0

# Logging
VITE_LOG_LEVEL=warn
VITE_ENABLE_ANALYTICS=true

# UuBookKit Integration (Production)
VITE_UUBOOKKIT_BASE_URL=https://uuapp.plus4u.net
VITE_UUBOOKKIT_BOOK_URI=${UUBOOKKIT_BOOK_URI}
